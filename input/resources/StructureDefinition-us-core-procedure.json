                "path": "Procedure",
                "definition": "\\-",
                "comment": "\\-",
                "constraint": [
                    {
                        "key": "us-core-7",
                        "severity": "error",
                        "human": "Performed SHALL be present if the status is 'completed' or 'in-progress'",
                        "expression": "(status='completed' or status='in-progress') implies performed.exists()"
                    }
                ],
                "mustSupport": false,
                "mapping": [
                    {
                        "identity": "argonaut-dq-dstu2",
                        "map": "Procedure"
